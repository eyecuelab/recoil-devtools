{
  "name": "@eyecuelab/recoil-devtools",
  "authors": [
    "joshwrn",
    "jeremybanka"
  ],
  "version": "0.0.0",
  "keywords": [
    "react",
    "recoil",
    "devtools"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eyecuelab/recoil-devtools.git"
  },
  "type": "module",
  "main": "dist/App.js",
  "files": [
    "dist"
  ],
